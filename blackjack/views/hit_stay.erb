<div>
  <% if session[:player][:total] < 21 && !session[:stay]%>
  <form class = "span1 inline-form" action = '/hit' method = 'post'>
    <button type="submit" class="btn btn-success btn-large" role="button" id = "hit">Hit</button>
  </form>
  <form class = "span1 inline-form" action = '/stay' method = 'post'>
    <button type = "submit" class="btn btn-danger btn-large" role="button" id = "stay">Stay</button>
  </form>
  <%end%>
</div>
